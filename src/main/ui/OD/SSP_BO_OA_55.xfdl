              <Band id="body">
                <Cell text="bind:BZPLC_ID" displaytype="expr:currow == 0 ? &quot;editcontrol&quot; : &quot;normal&quot;" edittype="none"/>
                <Cell col="1" text="bind:BZPLC_NM" displaytype="expr:currow == 0 ? &quot;combocontrol&quot; : &quot;normal&quot;" edittype="expr:currow == 0 ? &quot;combo&quot; : &quot;none&quot;" combodataset="ds_list02" combocodecol="BZPLC_ID" combodatacol="BZPLC_NM" combodisplaynulltext="전체" combotype="dropdown"/>
                <Cell col="2" text="bind:OPR_UNIT_ID" displaytype="expr:currow == 0 ? &quot;editcontrol&quot; : &quot;normal&quot;" edittype="expr:currow == 0 ? &quot;text&quot; : &quot;none&quot;"/>
