        <Grid id="grid_list01" taborder="3" left="20" top="98" right="20" autofittype="col" nodatatext="선택된 결과가 없습니다." autoenter="select" height="492" binddataset="ds_list01" oncelldblclick="btn_select_onclick" onenterdown="grid_list01_onenterdown" selecttype="row" font="12px/normal &quot;맑은 고딕&quot;">
