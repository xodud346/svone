        <Button id="btn_select" taborder="5" text="확인" width="81" right="400" textPadding="0px" onclick="btn_select_onclick" bottom="22" height="28"/>
        <Static id="sta_totcnt00" taborder="6" text="최대조회수" top="60" height="28" usedecorate="true" cssclass="sta_WF_txtR" right="252" width="60" onclick="Div00_sta_totcnt00_onclick" font="12px/normal &quot;맑은 고딕&quot;"/>
        <MaskEdit id="msk_searchCount" taborder="0" top="60" width="100" height="27" right="148" value="1000"/>
        <Button id="btn_clear" taborder="1" text="초기화" top="60" height="28" onclick="btn_clear_onclick" cssclass="btn_WF_default" width="60" right="85"/>
        <Static id="staticTitle" taborder="7" text="결재자 조회" left="20" top="20" height="21" cssclass="sta_WF_title04" width="300"/>
