          <Column id="MBR_BZPLC_ID" type="STRING" size="256"/>
          <Column id="MBR_BZPLC_NM" type="STRING" size="256"/>
          <Column id="BZPLC_ID" type="STRING" size="256"/>
          <Column id="BZPLC_NM" type="STRING" size="256"/>
