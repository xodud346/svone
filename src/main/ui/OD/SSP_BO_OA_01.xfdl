this.cmb_custTpAndCrtTp_onitemchanged = function(obj:nexacro.Combo,e:nexacro.ItemChangeEventInfo)
{
	var codeColumn = this.Div00.form.Div01.form.cmb_custTpAndCrtTp.value;
