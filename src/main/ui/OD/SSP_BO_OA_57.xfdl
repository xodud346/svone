        <Button id="btn_select" taborder="8" text="선택" width="81" textPadding="0px" onclick="btn_select_onclick" bottom="17" height="28" left="45%"/>
        <Button id="btn_clear" taborder="1" text="초기화" top="60" height="28" onclick="btn_clear_onclick" cssclass="btn_WF_default" width="60" right="85"/>
        <Static id="staticTitle" taborder="9" text="변경항목 조회" left="20" top="20" height="21" cssclass="sta_WF_title04" width="300"/>
        <Static id="sta_addedTotcnt" taborder="10" text="총 0건이 추가되었습니다." left="20" usedecorate="true" width="300" height="29" visible="false" top="362"/>
      </Layout>
