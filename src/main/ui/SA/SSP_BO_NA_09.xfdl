			break;

		case "btn_searchCpnExcptTgtPrdMulText30" :

			var retValue = retObj.textValue;
			
			if (retValue != "")
			{
				this.ds_searchPrdIdCheckList.clearData();
				
				var sTempValue = retValue.split(",");

				for (var x = 0; x < sTempValue.length; x++)
				{
