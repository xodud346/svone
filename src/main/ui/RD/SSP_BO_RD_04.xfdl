    this.ds_search.setColumn(nRow,"ROW_COUNT",                     this.lv_pageViewCnt);//현재 레코드 초기화셋팅
	this.ds_search.setColumn(nRow,"MALL_SPR_CD",                   this.mallSprCd);     // /*10:SSP, 20:RND*/
    this.div_paging.reload();
	this.fn_pageCallback();

    // 일자 날짜 초기화 Setting
