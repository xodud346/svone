{
	var btnEnableTreu   = {
							btn_updReq : this.btn_updReq
						  , btn_close  : this.btn_close
	                      };
	var btnVisibleFalse = {
							btn_rjct  : this.btn_rjct
						  , btn_consn : this.btn_consn
	                      };

	this.btn_updReq.set_text("취소");
