	|| (this.ds_master.getColumn(0,"ELC_CONT_APPR_STATS_CD") == "42" || this.ds_master.getColumn(0,"ELC_CONT_APPR_STATS_CD") == "52"))
	{
		strMsg = "ERRC000087"; // 취소하시겠습니까?
	}

	var result = this.fn_confirm(strMsg, "취소","", "question" ); 

    if(result == 0)
    {
        return false;
    }

	this.close("CLOSE");
};

