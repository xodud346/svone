this.sParamArg4     = "";
this.sParamArg5     = "";
this.lvchkColidDs   = "PRD_PRC$DLV_LT$VLD_STR_DT$VLD_END_DT";                        // 필수 입력사항 칼럼ID  예)  "commSmcd$commName"
