		//상세조회 시 readonly 처리
		this.stc_main.set_text("전용상품 노출판매가 상세");
 		this.div_form1.form.edt_prdId.set_readonly(true);
		this.div_form1.form.edt_bzplcId.set_readonly(true);
		this.div_form1.form.edt_salsDsgnSalsprc.set_readonly(true);
