	if(this.ds_popupList.rowcount == 1)
	{
		this.div_form1.form.edt_prdId.set_value(this.ds_popupList.getColumn(0,"PRD_ID_VIEW"));
		this.div_form1.form.edt_prdNm.set_value(this.ds_popupList.getColumn(0,"PRD_NM"));
