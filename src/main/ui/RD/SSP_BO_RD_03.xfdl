		return false;
	}
	// 국가코드 정합성 불일치시
	if(this.lv_ctryCdYn == "N")
	{
		this.fn_alert("ERRR000174","저장정보","원산지 코드","question"); // 존재하지 않는 &1 입니다.
		this.edt_ctrySprCd.setFocus();
