              <Static id="sta_totcnt" taborder="1" text="총 0건" left="0" top="40" width="180" height="25" usedecorate="true"/>
              <Grid id="grd_list" taborder="3" top="77" height="442" binddataset="ds_list" autofittype="col" cellsizebandtype="body" oncelldblclick="grd_list_oncelldblclick" autoenter="select" onenterdown="grd_list_onenterdown" oncellclick="grd_list_oncellclick" left="0" right="0" cellsizingtype="col" scrollbartype="auto fixed">
