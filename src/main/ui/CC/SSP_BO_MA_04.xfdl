        </ColumnInfo>
      </Dataset>
    </Objects>
    <Bind>
      <BindItem id="item0" compid="Div00_00.form.cbo_useYn" propid="text" datasetid="ds_useYn" columnid="COL_NM"/>
    </Bind>
  </Form>
