              <Static id="Static27" taborder="0" left="20" top="20" height="32" cssclass="sta_cm_box02L" text="" right="20"/>
              <Static id="Static26" taborder="1" text="상태" left="20" top="20" width="130" height="32" cssclass="sta_cm_box01L"/>
              <Combo id="cbo_useYn" taborder="2" text="" left="Static26:10" top="24" width="160" height="24" index="-1" innerdataset="ds_useYn" datacolumn="COL_NM" codecolumn="COL_VALUE" onitemchanged="Div00_00_cbo_useYn_onitemchanged"/>
            </Layout>
