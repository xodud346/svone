                    </Band>
                    <Band id="body">
                      <Cell text="expr:currow == 0 ? &quot;theme::default/images/grd_WF_search.png&quot; : &quot;&quot;" displaytype="expr:currow == 0 ? &quot;imagecontrol&quot; : &quot;normal&quot;"/>
                      <Cell col="1" text="bind:PGM_GRP_ID" textAlign="center" edittype="expr:currow == 0 ? &quot;text&quot; : &quot;none&quot;" displaytype="expr:currow == 0 ? &quot;editcontrol&quot; : &quot;normal&quot;"/>
