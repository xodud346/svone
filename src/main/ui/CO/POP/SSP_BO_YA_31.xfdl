              <Button id="btn_search" taborder="0" text="조회" top="41" height="28" onclick="btn_search_onclick" cssclass="btn_WF_require" width="60" right="0"/>
              <Static id="Static60_00" taborder="1" text="프로그램 그룹 조회" left="0" top="0" height="21" cssclass="sta_WF_title04" width="300"/>
              <Static id="sta_totcnt" taborder="2" text="(총 0건)" left="0" top="51" width="170" height="25" usedecorate="true"/>
              <Grid id="grd_list" taborder="4" left="0" top="79" height="442" binddataset="ds_pgmGrpInfo" autofittype="col" cellsizebandtype="allband" oncelldblclick="grd_list_oncelldblclick" oncellclick="grd_list_oncellclick" onenterdown="grd_list_onenterdown" autoenter="select" positionstep="0" right="0">
