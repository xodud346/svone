              <Combo id="cmb_rfiTpCd" taborder="1" left="140" top="58" width="150" height="24" index="-1" innerdataset="ds_rfiTpCd" datacolumn="COM_DTL_CD_NM" codecolumn="COM_DTL_CD"/>
