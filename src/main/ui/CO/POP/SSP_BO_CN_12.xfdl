            <Layout>
              <TextArea id="edt_qstCts" taborder="3" left="140" top="120" right="10" height="500"/>
              <Static id="Static65" taborder="10" left="0" top="115" height="610" cssclass="sta_cm_box02L" right="0"/>
              <Static id="Static07" taborder="8" left="0" top="84" height="32" cssclass="sta_cm_box02L" right="0" text=""/>
              <Static id="Static03" taborder="6" left="0" top="53" height="32" cssclass="sta_cm_box02L_tdt" right="0" text=""/>
              <Static id="Static00" taborder="0" text="자주하는 질문" left="0" top="22" height="30" cssclass="sta_WF_title01" onclick="Static00_onclick" width="300"/>
              <Static id="Static02" taborder="5" text="유형" left="0" top="53" width="130" height="32" cssclass="sta_cm_box01L_tdt_necessary" onclick="Div_top_info_Static02_onclick"/>
