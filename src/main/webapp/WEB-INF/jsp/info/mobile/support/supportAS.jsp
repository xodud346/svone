<script type="text/javascript">
$(document).ready(function(){

    // faq
    $('.faq-list .question a').on('click', function(e) {
        e.preventDefault();

        var $this = $(this),
            _col = $this.parents('li');

        _col.toggleClass('in');
        _col.siblings().removeClass('in').find('a em').text('열기');

        if (_col.hasClass('in')) {
            $this.find('em').text('닫기');
        } else {
            $this.find('em').text('열기');
        }
    });

});
</script>
